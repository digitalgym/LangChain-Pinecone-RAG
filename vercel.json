{
  "buildCommand": "pip install -r requirements-vercel.txt && python index_semantic_kernel.py",
  "builds": [
    {
      "src": "api/index_semantic_kernel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index_semantic_kernel.py"
    }
  ]
}